{
    "name": "drjele/doctrine-encrypt",
    "type": "symfony-bundle",
    "version": "v4.3.1",
    "description": "Doctrine encrypt type",
    "keywords": [
        "symfony",
        "doctrine",
        "encrypt",
        "decrypt"
    ],
    "homepage": "https://gitlab.com/drjele-symfony/doctrine-encrypt",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian Jeledintan",
            "email": "adrian.jeledintan@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "ext-openssl": "*",
        "doctrine/dbal": "3.*",
        "doctrine/orm": "3.*",
        "doctrine/persistence": "3.*",
        "drjele/symfony-console": "3.*",
        "symfony/config": "7.*"
    },
    "require-dev": {
        "drjele/symfony-phpunit": "5.*",
        "friendsofphp/php-cs-fixer": "3.*",
        "phpstan/phpstan": "1.*",
        "squizlabs/php_codesniffer": "3.*"
    },
    "autoload": {
        "psr-4": {
            "Drjele\\Doctrine\\Encrypt\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Drjele\\Doctrine\\Encrypt\\Test\\": "tests/"
        }
    },
    "scripts": {
        "install-hooks": [
            "[ $COMPOSER_DEV_MODE -eq 1 ] && rm -rf .git/hooks && ln -s ../dev/git-hooks .git/hooks || exit 0"
        ],
        "post-install-cmd": [
            "@install-hooks"
        ],
        "post-update-cmd": [
            "@install-hooks"
        ]
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}
